{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o src/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "react-script": "^2.0.5"
  },
  "description": "Tic Tac Toe",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebUp-dci-fbw7/tic-tac-toe-starter.git"
  },
  "author": "Zakaria Moslim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebUp-dci-fbw7/tic-tac-toe-starter/issues"
  },
  "homepage": "https://github.com/WebUp-dci-fbw7/tic-tac-toe-starter#readme"
}
